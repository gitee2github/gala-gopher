version = "1.0.0"
measurements:
(
    {
        table_name: "thread_cpu",
        entity_name: "thread",
        fields:
        (
            {
                description: "ID of the thread",
                type: "key",
                name: "pid",
            },
            {
                description: "PID id of thd process",
                type: "label",
                name: "tgid",
            },
            {
                description: "Thread offcpu times, unit is ns.",
                type: "gauge",
                name: "off_cpu_ns",
            },
            {
                description: "Thread migration count.",
                type: "gauge",
                name: "migration_count",
            }
        )
    }
)
